{
    "name": "cakedc/mixer",
    "description": "A plugin to discover and manage CakePHP plugins",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "plugin", "discovery", "update", "mixer"],
    "homepage": "https://github.com/CakeDC/mixer",
    "license": "MIT",
    "authors": [
        {
            "name": "CakeDC",
            "homepage": "http://www.cakedc.com",
            "role": "Author"
        },
        {
            "name": "Others",
            "homepage": "https://github.com/CakeDC/mixer/graphs/contributors"
        }
    ],
    "support": {
        "issues": "https://github.com/CakeDC/mixer/issues",
        "source": "https://github.com/CakeDC/mixer"
    },
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": "3.4.*",
        "composer/composer": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "CakeDC\\Mixer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeDC\\Mixer\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
